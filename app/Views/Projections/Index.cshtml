@model List<ActuarialApplications.Models.RiskFreeRate>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>

<p>ValueDate: @Model[0].ValueDate</p>

<table class="table">
    <thead>
        <tr>
            <th>
                Maturity
            </th>
            <th>
                Spot
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Maturity)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Value)
                </td>
            </tr>
        }
    </tbody>
</table>

